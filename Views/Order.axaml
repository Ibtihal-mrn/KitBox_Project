<!-- Choisir les dimensions-->
<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="KitBox_Project.Order"
        Title="KitBox_Project">
    <StackPanel>

        <Button Content="Home" Click="GoToMainWindow" HorizontalAlignment="Left" VerticalAlignment="Top" />

        <TextBlock Text="6. Récapitulatif" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="24" />

        <!-- Mettre un résumé des dimensions et de la commande -->

        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0,50,0,0">
            <Button Content="Back" Click="GoToChoice" Margin="5,0"/>
            <Button Content="Confirmer votre commande" Margin="5,0"/>
        </StackPanel>

    </StackPanel>
</Window>
