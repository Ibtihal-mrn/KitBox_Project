<!-- Choisir les dimensions-->
<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="KitBox_Project.Choice"
        Title="KitBox_Project">
    <StackPanel>

        <Button Content="Home" Click="GoToMainWindow" HorizontalAlignment="Left" VerticalAlignment="Top" />

        <TextBlock Text="5. Faites votre choix" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="24" />

        <!-- Mettre un résumé des dimensions -->

        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0,50,0,0">
            <Button Content="Back" Click="GoToDoor" Margin="5,0"/>
            <Button Content="Empiler un casier" Click="GoToHeight" Margin="5,0"/>
            <Button Content="Configurer une nouvelle armoire" Click="GoToSize" Margin="5,0"/>
            <Button Content="Finaliser la commande" Click="GoToOrder" Margin="5,0"/>
        </StackPanel>

    </StackPanel>
</Window>